{
  "name": "japaya",
  "publisher": "JustinOttesen",
  "displayName": "Japaya",
  "description": "VSCode plugin for Japaya - A Python injection framework for Java",
  "version": "1.1.2",
  "author": "Justin Ottesen",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/justinottesen/japaya.git",
    "directory": "editors/vscode/japaya"
  },
  "engines": {
    "vscode": "^1.100.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "japaya",
      "aliases": ["Japaya", "japaya"],
      "extensions": [".japaya"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "japaya",
      "scopeName": "source.japaya",
      "path": "./syntaxes/japaya.tmLanguage.json"
    },
    {
      "scopeName": "python.injection.japaya",
      "path": "./syntaxes/python.injection.japaya.json",
      "injectTo": [ "source.japaya" ]
    }]
  }
}
